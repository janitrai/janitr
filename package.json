{
  "name": "janitr",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.58.1",
    "prettier": "^3.8.1"
  },
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write . --ignore-unknown && ruff format .",
    "format:check": "prettier --check . --ignore-unknown && ruff format --check .",
    "simpledoc": "node ~/SimpleDoc/dist/bin/simpledoc.js",
    "simpledoc:check": "node ~/SimpleDoc/dist/bin/simpledoc.js check"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,tsx,json,css,md,html,yaml,yml}": "prettier --write --ignore-unknown",
    "*.py": "ruff format"
  }
}
